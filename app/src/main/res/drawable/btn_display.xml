<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#273E89" />
            <stroke android:width="2dp" android:color="#273E89" />
            <corners android:bottomLeftRadius="12dp" android:bottomRightRadius="12dp" android:topLeftRadius="12dp" android:topRightRadius="12dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:angle="270" android:endColor="#fff" android:startColor="#fff" />
            <stroke android:width="2dp" android:color="#273E89" />
            <corners android:bottomLeftRadius="12dp" android:bottomRightRadius="12dp" android:topLeftRadius="12dp" android:topRightRadius="12dp" />
        </shape>
    </item>
</selector>
